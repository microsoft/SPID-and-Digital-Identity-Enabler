            <OutputClaim ClaimTypeReferenceId="fiscalNumber" PartnerClaimType="fiscalNumber" />
            <OutputClaim ClaimTypeReferenceId="vatNumber" PartnerClaimType="vatNumber" />
            <OutputClaim ClaimTypeReferenceId="idCard" PartnerClaimType="idCard" />
            <OutputClaim ClaimTypeReferenceId="mobilePhone" PartnerClaimType="mobilePhone" />
